	function Go(destination)
	{
		if(destination=='home'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/home";}
		else if(destination=='inicio'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/hpd";}		
		else if(destination=='googleapps'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/google-apps";}
		else if(destination=='trucosyconsejos'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/trucosyconsejos";}
		else if(destination=='movilidad'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/movilidad";}
		else if(destination=='exito'){window.parent.location.href="https://sites.google.com/a/bbva.com/portaldeayuda/casos-de-exito";}

		return false;
	}

	gadgets.util.registerOnLoadHandler(renderSelected);

	function renderSelected(){
		resetStyles();
	}
	function resetStyles (){

			jQuery('.homefirst').removeClass("selectedLeft");
			jQuery('.exito').removeClass("selected");
			jQuery('.apps').removeClass("selected");
			jQuery('.trucos').removeClass("selected");
			jQuery('.iniciate').removeClass("selected");
			jQuery('.movilidad').removeClass("selected");			

		return datospagina();
	}

	function datospagina()
	{

		var parametros = gadgets.util.getUrlParameters()["parent"];

		if (parametros.indexOf("home")!=-1 || parametros == "https://sites.google.com/a/bbva.com/portaldeayuda/")
		{
			jQuery('.homefirst').addClass("selectedLeft");
			return;
		}else if(parametros.indexOf("casos-de-exito")!=-1){
			jQuery('.exito').addClass("selected");
			return;
		} else if(parametros.indexOf("google-apps")!=-1){
			jQuery('.apps').addClass("selected");
			return;
		} else if(parametros.indexOf("trucosyconsejos")!=-1){
			jQuery('.trucos').addClass("selected");
			return;
		} else if((parametros.indexOf("iniciate")!=-1) || (parametros.indexOf("hpd")!=-1) &&(parametros.indexOf("google-apps")==-1)){			
			jQuery('.iniciate').addClass("selected");
			return;
		}else if(parametros.indexOf("movilidad")!=-1){
			jQuery('.movilidad').addClass("selected");
			return;
		}
	}



