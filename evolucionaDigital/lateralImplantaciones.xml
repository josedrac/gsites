		<div class="container">
        <div class="title">Histórico de Implantaciones</div>
        <div class="form">
            <div class="element-year">
                <label class="name">Año: </label>
                <select name="anyo" id="yearSelector">
                    <option value="0">Todos</option>
                    <option value="2013">2013</option>
                    <option value="2014">2014</option>
                    <option value="2015">2015</option>
                    <option value="2016">2016</option>
                </select>
            </div>
            <div class="element-month">
                <label class="name">Mes: </label>
                <select id="monthSelector" name="mes">
                    <option value="0">Cualquiera</option>
                    <option value="1">Enero</option>
                    <option value="2">Febrero</option>
                    <option value="3">Marzo</option>
                    <option value="4">Abril</option>
                    <option value="5">Mayo</option>
                    <option value="6">Junio</option>
                    <option value="7">Julio</option>
                    <option value="8">Agosto</option>
                    <option value="9">Septiembre</option>
                    <option value="10">Octubre</option>
                    <option value="11">Noviembre</option>
                    <option value="12">Diciembre</option>
                </select>
            </div>
        </div>
        <div class="subtitle">
            <button onclick='javascript:Go();' class="btn">Ver</button>
        </div>
    </div>
    </div>
    <script type="text/javascript">

        function Go() {
            var elementYearSelector = document.getElementById('yearSelector');
            var elementMonthSelector = document.getElementById('monthSelector');
            var selectedYear = elementYearSelector.options[elementYearSelector.selectedIndex].value;
            var selectedMonth = elementMonthSelector.options[elementMonthSelector.selectedIndex].value;
            window.open("https://sites.google.com/a/bbva.com/evolucionadigital/implantaciones"+"?anyo="+selectedYear+"&mes="+selectedMonth,"_parent");
            return false;
        }

        $( "#yearSelector" ).selectmenu({width: 190});

        $( "#monthSelector" ).selectmenu({width: 190});

    </script>

