	function Go(destination)
	{
		if(destination=='home'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/";}
		else if(destination=='opcion1'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/ayuda-a-digitalizar";}
		else if(destination=='opcion2'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/decalogo";}
		else if(destination=='opcion3'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/novedades";}
		else if(destination=='opcion4'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/operativa-por-canal";}
		else if(destination=='opcion5'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/pruebalo-antes";}
    else if(destination=='opcion6'){window.parent.location.href="https://sites.google.com/a/bbva.com/evolucionadigital/tu-opinion";}

		return false;
	}

