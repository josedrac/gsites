	function resetStyles (){

			jQuery('.homefirst').removeClass("selectedLeft");
			jQuery('.opcion1').removeClass("selected");
			jQuery('.opcion2').removeClass("selected");
			jQuery('.opcion3').removeClass("selected");
			jQuery('.opcion4').removeClass("selected");
