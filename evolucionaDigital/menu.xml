      jQuery('.opcion6').removeClass("selected");


		return datospagina();
	}

	function datospagina()
	{

		var parametros = gadgets.util.getUrlParameters()["parent"];

		if (parametros.indexOf("home")!=-1 || parametros == "https://sites.google.com/a/bbva.com/portaldeayuda/")
		{
			jQuery('.homefirst').addClass("selectedLeft");
			return;
		}else if(parametros.indexOf("ayuda-a-digitalizar")!=-1){
			jQuery('.opcion1').addClass("selected");
			return;
		} else if(parametros.indexOf("decalogo")!=-1){
			jQuery('.opcion2').addClass("selected");
			return;
		} else if(parametros.indexOf("novedades")!=-1){
			jQuery('.opcion3').addClass("selected");
			return;
		} else if((parametros.indexOf("ayuda-a-digitalizar")!=-1) || (parametros.indexOf("ayuda-a-digitalizar")!=-1) &&(parametros.indexOf("ayuda-a-digitalizar")==-1)){
			jQuery('.opcion1').addClass("selected");
			return;
		}else if(parametros.indexOf("operativa-por-canal")!=-1){
